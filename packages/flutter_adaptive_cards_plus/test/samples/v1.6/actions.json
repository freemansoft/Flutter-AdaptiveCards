{
  "type": "AdaptiveCard",
  "version": "1.6",
  "body": [
    {
      "type": "TextBlock",
      "text": "New Actions Examples",
      "size": "Medium",
      "weight": "Bolder"
    },
    {
       "type": "Input.Text",
       "id": "myText",
       "placeholder": "Type something to reset..."
    }
  ],
  "actions": [
