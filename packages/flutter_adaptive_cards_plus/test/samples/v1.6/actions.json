            { "type": "TextBlock", "text": "This is a popover!" }
         ]
      }
    },
    {
       "type": "Action.OpenUrlDialog",
       "title": "Open URL Dialog",
       "url": "https://google.com"
    },
    {
       "type": "Action.InsertImage",
       "title": "Insert Image"
    }
  ]
}
